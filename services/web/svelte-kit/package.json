		"@sveltejs/kit": "^1.20.4",
		"@vitest/coverage-v8": "^1.0.4",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"tailwindcss": "^3.3.3",
		"vite": "^4.4.2",
		"vitest": "^1.0.4"
	},
	"type": "module",
	"dependencies": {
		"argon2": "^0.31.2",
