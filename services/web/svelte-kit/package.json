{
	"name": "pocketbot",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
